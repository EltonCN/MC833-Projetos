{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/Projeto 1/src"
            ],
            "defines": [
                "_POSIX_C_SOURCE=200112L"
